# Environment
NODE_ENV=production
PORT=3006

# Database (overridden in docker-compose, but needed for local dev)
DATABASE_URL="postgresql://postgres:postgres@localhost:5435/warehouse_db?schema=public"

# Docker Postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change_me_in_production
POSTGRES_DB=warehouse_db

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_ACCESS_EXPIRATION=15m
JWT_REFRESH_EXPIRATION=7d

# CORS
CORS_ORIGIN=https://business.medlife.tj
